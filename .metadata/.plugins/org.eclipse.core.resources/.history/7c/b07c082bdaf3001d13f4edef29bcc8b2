import java.util.Scanner;
	
 
public class Media {
	
	String[] alunos = {};
	
	
	 void listarAlunos( ) {
		
	
		
   for(int indice = 0; indice < alunos.length; indice++) { 
	   alunos[indice] = Scanner.next();

	   System.out.println("NOTAS DO ALUNO(A):  " + alunos[indice]);
	   
   		}
		
   		System.out.println("TOTAL DE ALUNOS:" + alunos.length);	
		
	}

}

    