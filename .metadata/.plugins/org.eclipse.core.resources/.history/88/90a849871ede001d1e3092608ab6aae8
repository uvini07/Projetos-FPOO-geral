import java.util.Scanner;

	public class WhileSentinela {
		Scanner scanner = new Scanner (System.in);
		
		
	public void Sentinela() {
		
		int sentinela = 0;
		int soma = 0;
		while (sentinela >=0); {
			
			System.out.println("DIGITE UM VALOR: ");
	        sentinela = scanner.nextInt();
			soma = soma + sentinela;
			
			
			
		}
	}
}