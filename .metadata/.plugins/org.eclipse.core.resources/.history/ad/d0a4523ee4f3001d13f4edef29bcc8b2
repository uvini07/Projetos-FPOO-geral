public class Valor {
    public static void main(String[] args) {
        int[] valores = new int[10];
        int[] valores2 = new int[10];

        // Preencher o array "valores" com valores numéricos inteiros
        valores[0] = 1;
        valores[1] = 2;
        valores[2] = 3;
        valores[3] = 4;
        valores[4] = 5;
        valores[5] = 6;
        valores[6] = 7;
        valores[7] = 8;
        valores[8] = 9;
        valores[9] = 10;

        // Multiplicar cada valor do array "valores" por três e armazenar no array "valores2"
        for (int i = 0; i < valores.length; i++) {
            valores2[i] = valores[i] * 3;
        }

        // Exibir os valores de ambos os arrays na tela
        System.out.println("Valores do array 'valores':");
        for (int i = 0; i < valores.length; i++) {
            System.out.println(valores[i]);
        }

        System.out.println("\nValores do array 'valores2':");
        for (int i = 0; i < valores2.length; i++) {
            System.out.println(valores2[i]);
        }
    }
}